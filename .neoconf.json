{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true,
      "runtime": true,
      "types": true
    },
    "pathStrict": false,
    "lspconfig": true
  },
  "neoconf": {
    "live_reload": true,
    "filetype_jsonc": true,
    "plugins": {
      "lua_ls": {
        "enabled": true
      },
      "jsonls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.runtime.pathStrict": false,
      "Lua.runtime.path": [
        "?.lua",
        "?/init.lua",
        "${workspace}/home/modules/neovim/files/?.lua",
        "${workspace}/home/modules/neovim/files/?/init.lua"
      ]
    }
  }
}
